<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="AES_Improved_wrapper"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
AES_Improved_wrapper
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="AES_Improved_wrapper">
        <sdx:configuration sdx:name="AES_Improved_wrapper"
                           sdx:displayName="AES_Improved_wrapper"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>AES_Improved_wrapper</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="AES_Improved_wrapper/boot/AES_Improved_wrapper.bif"
                            sdx:readme="AES_Improved_wrapper/boot/generic.readme"
                            sdx:qemuArguments="AES_Improved_wrapper/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="AES_Improved_wrapper/boot/AES_Improved_wrapper.bif"
                            sdx:readme="AES_Improved_wrapper/boot/generic.readme"
                            sdx:qemuArguments="AES_Improved_wrapper/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="AES_Improved_wrapper/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="AES_Improved_wrapper/standalone_domain/bspinclude/include"
                        sdx:bspConfig="AES_Improved_wrapper/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
